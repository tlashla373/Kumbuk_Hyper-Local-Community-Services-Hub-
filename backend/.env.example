# ==========================================
# KUMBUK BACKEND ENVIRONMENT CONFIGURATION
# ==========================================

# ==========================================
# GOOGLE CLOUD / VERTEX AI
# ==========================================

# Your Google Cloud Project ID
GOOGLE_CLOUD_PROJECT=your-gcp-project-id

# Vertex AI Location (default: us-central1)
VERTEX_AI_LOCATION=us-central1

# ==========================================
# GEMINI-PRO LLM CONFIGURATION
# ==========================================

# Model name (default: gemini-pro)
GEMINI_MODEL=gemini-pro

# Temperature (0.0-1.0, lower = more deterministic)
GEMINI_TEMPERATURE=0.2

# Max output tokens
GEMINI_MAX_TOKENS=1024

# Top-p sampling
GEMINI_TOP_P=0.8

# Top-k sampling
GEMINI_TOP_K=40

# ==========================================
# FIREBASE CONFIGURATION (Optional)
# ==========================================

FIREBASE_PROJECT_ID=your-firebase-project-id
FIREBASE_API_KEY=your-firebase-api-key

# ==========================================
# NEO4J CONFIGURATION (Optional)
# ==========================================

NEO4J_URI=neo4j+s://your-neo4j-instance.databases.neo4j.io
NEO4J_USER=neo4j
NEO4J_PASSWORD=your-neo4j-password

# ==========================================
# APPLICATION CONFIGURATION
# ==========================================

# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME=Kumbuk AI Agent System
VERSION=1.0.0

# CORS Origins (comma-separated)
BACKEND_CORS_ORIGINS=*

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# ==========================================
# NOTES
# ==========================================
# 1. Copy this file to .env in the backend/ directory
# 2. Update the values with your actual credentials
# 3. NEVER commit .env file to git (already in .gitignore)
# 4. For GCP authentication, also set up Application Default Credentials:
#    gcloud auth application-default login
